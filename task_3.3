/*
Задание: Сделайте функцию, которая параметрами принимает 2 числа.
Если эти числа равны - пусть функция вернет true, а если не равны - false.
*/


import java.util.Scanner;

public class Main {

  public static void main(String[] args) {
    System.out.println("Введите первый параметр: ");
    Scanner firsNum = new Scanner(System.in);
    int a = firsNum.nextInt();
    System.out.println("Введите второй параметр: ");
    Scanner secondNum = new Scanner(System.in);
    int b = secondNum.nextInt();
    if ((a > b) || (b > a)) {
      System.out.println(false);
    }
    if (a == b) {
      System.out.println(true);

    }
  }
}

