public class Main {

  static int[] nums;

  static int i = 0;

  public static void main(String[] args) {
    nums = new int[]{4, 2, 6, 2, 65};
    i = nums.length;
    queue(i - 1);
  }

  private static void queue(int i) {

    if (i == 0) {
      System.out.println("[" + i + "] - " + nums[i]);
      return;
    } else {
      queue(i - 1);
      System.out.println("[" + i + "] - " + nums[i]);
    }
  }
}
