public class Main {
    public static void main(String[] args) {
        Dog maks = new Dog("Жужик", 4);
        maks.name();
        maks.age();
    }
}
public class Dog {

    public String name;
    public int age;

    public Dog(String name, int age) {
        this.name = name;
        this.age = age;
    }


    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getAge() {
        return age;
    }

    public void setAge(int age) {
        this.age = age;
    }


    public void age() {
        System.out.println("Мой возраст ");
    }

    public void name() {
        System.out.println("Меня зовут ");
    }
}
